{
  "private": true,
  "name": "vulmix-app",
  "scripts": {
    "dev": "tsc ./vulmix.config.ts --outDir ./.vulmix && mix watch --mix-config=.vulmix/laravel-mix/webpack.mix.js --hot",
    "prod": "mix --mix-config=.vulmix/laravel-mix/webpack.mix.js --production",
    "serve": "npm run prod && npx http-server -p 8000 -a localhost _dist --gzip --proxy http://localhost:8000?"
  },
  "dependencies": {
    "vulmix": "0.6.0-beta"
  }
}
