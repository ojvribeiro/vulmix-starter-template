{
  "private": true,
  "name": "vulmix-app",
  "scripts": {
    "prepare": "tsc ./vulmix.config.ts --outDir ./.vulmix",
    "dev": "node ./.vulmix/cli.js",
    "prod": "tsc ./vulmix.config.ts --outDir ./.vulmix && mix --mix-config=.vulmix/laravel-mix/webpack.mix.js --production",
    "serve": "npm run prod && npx http-server -p 8000 -a localhost _dist --gzip --proxy http://localhost:8000?"
  },
  "dependencies": {
    "vulmix": "preview"
  }
}
